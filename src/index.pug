                        .swiper-slide.d-flex.justify-content-center.align-items-center
                            img(src="images/partners/excellench.png", alt="excellench")
                        .swiper-slide.d-flex.justify-content-center.align-items-center
                            img(src="images/partners/emphasoft.svg", alt="emphasoft")
